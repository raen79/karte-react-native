{
  "name": "@react-native-karte/visual-tracking",
  "version": "0.1.0",
  "description": "KARTE SDK Visual tracking for React Native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "RNKRTVisualTracking.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "prepare": "bob build",
    "bootstrap": "yarn"
  },
  "aa": {
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "karte",
    "visual-tracking",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plaidev/karte-react-native.git",
    "directory": "packages/visual-tracking"
  },
  "author": "PLAID, Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/plaidev/karte-react-native/issues"
  },
  "homepage": "https://github.com/plaidev/karte-react-native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-builder-bob": "*",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "@react-native-karte/core": "^0.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
